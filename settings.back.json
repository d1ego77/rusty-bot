{
	//Window
	// Menubar Visibility (VSCODE)
	"window.menuBarVisibility": "toggle",
	"window.density.editorTabHeight": "compact",
	//Editor
	"editor.tabSize": 2,
	"editor.fontSize": 15,
	"editor.fontWeight": "500",
	"editor.fontLigatures": true,
	"editor.fontFamily": "Iosevka, 'JetBrains Mono', 'monospace', monospace",
	"editor.wordWrap": "bounded",
	"editor.wrappingIndent": "same",
	"editor.lineNumbers": "relative",
	"editor.rulers": [
		120
	],
	"editor.wordWrapColumn": 150,
	"editor.folding": false,
	"editor.insertSpaces": false,
	"editor.smoothScrolling": false,
	"editor.minimap.enabled": false,
	"editor.detectIndentation": true,
	"editor.suggestSelection": "first",
	"editor.scrollBeyondLastLine": false,
	"editor.renderControlCharacters": false,
	"editor.unicodeHighlight.ambiguousCharacters": false,
	"editor.quickSuggestionsDelay": 0,
	"editor.glyphMargin": false,
	"editor.scrollbar.horizontal": "hidden",
	"editor.scrollbar.vertical": "hidden",
	"editor.hideCursorInOverviewRuler": true,
	"editor.unicodeHighlight.allowedCharacters": {
		"а": true,
		"с": true,
		"Т": true,
		"б": true,
		"е": true
	},
	"workbench.startupEditor": "none",
	"window.openFilesInNewWindow": "off",
	"workbench.sideBar.location": "left",
	// Terminal
	"terminal.integrated.fontFamily": "'JetBrains Mono', 'monospace', monospace",
	"terminal.integrated.fontSize": 14,
	"terminal.integrated.tabs.enabled": false,
	//Explorer
	"explorer.confirmDelete": false,
	"explorer.compactFolders": false,
	"explorer.confirmDragAndDrop": false,
	//Emmet
	"emmet.triggerExpansionOnTab": true,
	"emmet.showExpandedAbbreviation": "never",
	//Formatters
	"editor.inlineSuggest.enabled": true,
	// Files settings
	"files.associations": {
		"*.scss": "postcss",
		"*.module.scss": "postcss"
	},
	//Rust Formatter
	"rust-analyzer.inlayHints.typeHints.enable": false,
	"[rust]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "rust-lang.rust-analyzer",
		"editor.semanticHighlighting.enabled": false
	},
	//Breadcrumbs Off
	"breadcrumbs.enabled": false,
	// Java/Typescript
	"javascript.updateImportsOnFileMove.enabled": "always",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"typescript.preferences.quoteStyle": "double",
	"javascript.preferences.quoteStyle": "double",
	"javascript.format.semicolons": "insert",
	"typescript.format.semicolons": "insert",
	"typescript.suggest.paths": false,
	"javascript.suggest.paths": false,
	//QuickGrep
	"quickgrep.grep": "Ripgrep",
	// Telemetry (VSCODE)
	"telemetry.telemetryLevel": "off",
	"vim.handleKeys": {
		"g": true,
	},
	"vim.leader": "<space>",
	"vim.easymotion": true,
	"vim.incsearch": true,
	"vim.useSystemClipboard": true,
	"vim.useCtrlKeys": true,
	"vim.hlsearch": true,
	"vim.highlightedyank.enable": true,
	"vim.insertModeKeyBindings": [
		{
			"before": [
				"j",
				"k"
			],
			"after": [
				"<Esc>"
			]
		}
	],
	"vim.normalModeKeyBindingsNonRecursive": [],
	"vim.normalModeKeyBindings": [
		{
			"before": [
				"<leader>",
				"f",
				"p"
			],
			"commands": [
				"quickgrep.action"
			]
		},
		{
			"before": [
				"<leader>",
				"s",
				"a"
			],
			"commands": [
				"editor.action.selectAll"
			]
		},
		{
			"before": [
				"<leader>",
				"g",
				"d"
			],
			"commands": [
				"editor.action.revealDefinition"
			]
		},
		{
			"before": [
				"<leader>",
				"w"
			],
			"commands": [
				"workbench.action.files.save"
			]
		},
		{
			"before": [
				"<leader>",
				"e"
			],
			"commands": [
				"workbench.action.toggleSidebarVisibility"
			]
		},
		{
			"before": [
				"<leader>",
				"q"
			],
			"commands": [
				"workbench.action.closeActiveEditor"
			]
		},
		{
			"before": [
				"<leader>",
				"h"
			],
			"commands": [
				":nohl"
			]
		},
		{
			"before": [
				"<leader>",
				"s",
				"v"
			],
			"commands": [
				"workbench.action.splitEditor"
			]
		},
		{
			"before": [
				"<leader>",
				"s",
				"h"
			],
			"commands": [
				"workbench.action.splitEditorDown"
			]
		},
		{
			"before": [
				"<leader>",
				"t",
				"o"
			],
			"commands": [
				":tabnew"
			]
		},
		{
			"before": [
				"<leader>",
				"t",
				"x"
			],
			"commands": [
				":tabclose"
			]
		},
		{
			"before": [
				"<S-h>"
			],
			"commands": [
				":tabp"
			]
		},
		{
			"before": [
				"<S-l>"
			],
			"commands": [
				":tabnext"
			]
		},
		{
			"before": [
				"<leader>",
				"f",
				"f"
			],
			"commands": [
				"workbench.action.quickOpen"
			]
		},
		{
			"before": [
				"<leader>",
				"f",
				"b"
			],
			"commands": [
				"workbench.action.showAllEditors"
			]
		},
		{
			"before": [
				"<leader>",
				"f",
				"s"
			],
			"commands": [
				"quickgrep.action"
			]
		},
		{
			"before": [
				"<leader>",
				"r",
				"n"
			],
			"commands": [
				"editor.action.rename"
			]
		},
		{
			"before": [
				"<leader>",
				"a",
				"f"
			],
			"commands": [
				"editor.action.autoFix"
			]
		},
		{
			"before": [
				"<leader>",
				"r",
				"a"
			],
			"commands": [
				"editor.action.rename"
			]
		},
		{
			"before": [
				"<leader>",
				"f",
				"m"
			],
			"commands": [
				"editor.action.format"
			]
		},
		{
			"before": [
				"<leader>",
				"c",
				"a"
			],
			"commands": [
				"editor.action.quickFix"
			]
		},
		{
			"before": [
				"<S-k>"
			],
			"commands": [
				"editor.action.showHover"
			]
		}
	],
	"vim.visualModeKeyBindings": [
		{
			"before": [
				">"
			],
			"commands": [
				"editor.action.indentLines"
			]
		},
		{
			"before": [
				"<"
			],
			"commands": [
				"editor.action.outdentLines"
			]
		},
		{
			"before": [
				"<S-j>"
			],
			"commands": [
				"editor.action.moveLinesDownAction"
			]
		},
		{
			"before": [
				"<S-k>"
			],
			"commands": [
				"editor.action.moveLinesUpAction"
			]
		}
	],
	"github.copilot.enable": {
		"*": false,
		"plaintext": false,
		"markdown": false,
		"scminput": false,
		"rust": false
	},
	"editor.occurrencesHighlight": "off",
	"editor.selectionHighlight": false,
	"editor.guides.highlightActiveIndentation": false,
	"editor.guides.indentation": true,
	"editor.cursorSmoothCaretAnimation": "off",
	"editor.cursorBlinking": "phase",
	// "workbench.activityBar.location": "hidden"313131313131
	"workbench.colorCustomizations": {
		//   //   // "editorIndentGuide.background": "#d0d0d0",
		"editor.foreground": "#313131",
		//"editor.background": "#ebe9e0",
		"editor.background": "#efece0",
		//"editor.background": "#efece0",
		//   //   //   //   //   "editor.foreground": "#333338",
		//"editorCursor.background": "#faf6f6",
		//"editorCursor.foreground": "#a86372",
		"editorCursor.foreground": "#406Bf4",
		"editorCursor.background": "#faf6f6",
		//"editorCursor.foreground": "#c2bda7",
		//   //   "editor.lineHighlightBackground": "#ebe9e0",
		//   //   "editor.lineHighlightBorder": "#e4ded3",
		"editor.lineHighlightBackground": "#ebe9e0",
		"editor.lineHighlightBorder": "#e4ded3",
		// "editorBracketHighlight.foreground1": "#d18b8b",
		// "editorBracketHighlight.foreground2": "#d18b8b",
		// "editorBracketHighlight.foreground3": "#d18b8b",
		// "editorBracketHighlight.foreground4": "#d18b8b",
		// "editorBracketHighlight.foreground5": "#d18b8b",
		// "editorBracketHighlight.foreground6": "#d18b8b",
	},
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": [
					//"entity.name.type",
					//"entity.name.namespace",
					//"keyword.operator"
					"variable.language.self.rust",
				],
				"settings": {
					"foreground": "#7A3E9D"
				}
			},
			// {
			//   "scope": [
			//     //  "variable.language.self.rust",
			//     // "keyword.control"
			//     // "constant"
			//   ],
			//   "settings": {
			//     "foreground": "#082a78",
			//     "fontStyle": ""
			//   }
			// },
			{
				"scope": [
					"comment",
					"punctuation.definition.comment"
				],
				"settings": {
					"foreground": "#9e4a4a"
				}
			},
			{
				"scope": [
					"variable.language.self",
					"variable.language.this",
					"entity.name.type"
				],
				"settings": {
					"foreground": "#227a76"
				}
			},
			{
				"scope": [
					//"keyword",
					"keyword.control",
					"storage.modifier",
					"storage.type",
				],
				"settings": {
					//"foreground": "#1e1e7a"
					"foreground": "#242b89"
				}
			},
			{
				"scope": [
					"keyword.operator"
				],
				"settings": {
					"foreground": "#736b58"
				}
			},
			{
				"scope": [
					"entity.name",
				],
				"settings": {
					"foreground": "#313131"
				}
			},
			{
				"scope": [
					"entity.name.function",
					"entity.name.tag",
					"meta.function-call"
				],
				"settings": {
					"foreground": "#325CC0"
				}
			},
		],
	},
	"editor.formatOnSave": true,
	"powermode.presets": "exploding-rift",
	"editor.bracketPairColorization.enabled": false,
	"workbench.colorTheme": "chalice-color-theme-light",
}